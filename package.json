{
  "name": "vue3-admin",
  "version": "0.0.2",
  "author": "若尘",
  "homepage": "",
  "description": "a seed with vue standard boilerplate project",
  "scripts": {
    "bootstrap": "pnpm install",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "start": "pnpm run --filter '*' start",
    "start:logger": "pnpm -C ./packages/logger start",
    "start:indexedDbHelper": "pnpm -C ./packages/IndexedDbHelper start",
    "start:itools": "pnpm -C ./packages/itools start",
    "start:generator": "pnpm -C ./packages/generator start",
    "dev": "vite --mode development",
    "dev:open": "vite --open --mode development",
    "build:dev": "vite build --mode development",
    "build:pro": "vite build --mode production",
    "preview": "npm run build:dev && vite preview --open",
    "plop": "plop",
    "typecheck": "npx vue-tsc --noEmit",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "lint:css": "stylelint  src/**/*.{vue,scss,css,sass,less} --fix",
    "prettier": "prettier --write \"src/**/*.{vue,js,jsx,ts,tsx,json,css,scss,less,md}\"",
    "deps:update": "npx taze -w",
    "deps:update:major": "npx taze major -w",
    "deps:update:minor": "npx taze minor -w",
    "deps:update:patch": "npx taze patch -w",
    "commit": "cz",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf **/node_modules",
    "api:gen": "node ./src/utils/api-gen/index.ts"
  },
  "dependencies": {
